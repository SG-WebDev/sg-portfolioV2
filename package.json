{
  "name": "sg-portfoliov2",
  "version": "1.0.0",
  "description": "Front-end developer portfiolo page",
  "main": "-",
  "scripts": {
    "scss:mobile": "node-sass ./src/scss/mobile.scss --source-map true -o -r ./dist/css/mobile.css --output-style compressed",
    "scss:desktop": "node-sass ./src/scss/desktop.scss --source-map true -o -r ./dist/css/desktop.css --output-style compressed",
    "js:terser": "terser ./src/js/* --output dist/js/index.js",
    "dev": "npm-run-all -p scss:* js:*",
    "watch": "watch \"npm run dev\" ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SG-WebDev/sg-portfolioV2.git"
  },
  "author": "SG-WebDev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SG-WebDev/sg-portfolioV2/issues"
  },
  "homepage": "https://sgrobelny.dev",
  "devDependencies": {
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "terser": "^5.2.1",
    "watch": "^1.0.2"
  }
}
